citya=np.array( [30, 32, 31, 29, 28, 27, 26])
cityb=np.array( [35, 34, 36, 33, 32, 31, 30])
cityc=np.array( [25, 26, 27, 28, 29, 30, 31])
cityd=np.array( [22, 23, 24, 25, 26, 27, 28])
x=np.sum(citya)
x=x/7
print(x)
y=np.sum(cityb)
z=np.sum(cityc)
p=np.sum(cityd)
y=y/7
z=z/7
p=p/7
print(y)
print(z)
print(p)
maxa=np.max(citya)
print(maxa)
maxb=np.max(cityb)
print(maxb)
maxc=np.max(cityc)
print(maxc)
maxd=np.max(cityd)
print(maxd)
